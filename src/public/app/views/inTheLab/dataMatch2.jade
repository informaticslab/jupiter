.row
	.div(ng-class='getContentWidth()')
		.content_block
			span.visible-md.visible-lg
				.subnav
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			span.visible-sm
				.subnav_sm
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li.active
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			.detail_header 
				.detail_header
					.detail_header_text_contain_lg
						.detail_header_text
							| Data Harmonization (Alpha)
			.top_block_faq
				.vertical_spacing_less
				.row
					|PREVIEW - Combined Data Sets
				.row
					#grid2.grid(ui-grid='{columnDefs: columnDefs, data:mergedDatasets, enableGridMenu: true, enableSelectAll: true}' ui-grid-auto-resize='' ui-grid-selection='' ui-grid-exporter='')		
				.vertical_spacing_more
				.row
					button.btn.btn-success.pull-left(type='button' ng-click='backToMatch()' ) Back
					button.btn.btn-success.pull-right(type='button' ng-click='showValueSet()' ng-show='validDataSets') Value Set Rules
								

	.div(ng-class='getSidebarWidth()')
		.hx_side(ng-show='!showSidebar' ng-click='toggleSidebar()')
			div(ng-Historybar)
