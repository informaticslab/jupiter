.row
	.div(ng-class='getContentWidth()')
		.content_block
			span.visible-md.visible-lg
				.subnav
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			span.visible-sm
				.subnav_sm
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li.active
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			.detail_header 
				.detail_header
					.detail_header_text_contain_lg
						.detail_header_text
							| Universal Viewer: Ordered by Activity Type
					.share_button_linkage
						//These lines, 173 - 187, were added 
						//Single button 
						.btn-group
							button.btn.btn-default.dropdown-toggle(type='button', data-toggle='dropdown')
								span.glyphicon.glyphicon-share
								| &nbsp;Share &nbsp;&nbsp;
								span.caret
							ul.dropdown-menu(role='menu')
								li
									a(href='mailto:?subject=CDC iSurv Portal: Universal Viewer III&body={{uvBlurb}}') Email

								li
									a(href='' ng-click='share(["CDC iSurv Portal: Universal Viewer III: ", "#/inTheLab/inTheLabArc"])' id='fb-root') Facebook

								li
									a(target='_blank' href='http://twitter.com/home?status=CDC iSurv Portal: Universal Viewer III:{{uvBlurb}}') Twitter
						//end added lines 
			// end detail header
			.top_block2
				img(src='img/ajax-loader.gif', alt='Content is Loading', ng-show='true', id='loadingImg')
				div#tooltip
					P
						strong Name: &nbsp;&nbsp;
						stat_label(id='name')
					P
						strong Type: &nbsp;&nbsp;
						stat_label(id='type')
					P
						strong Relationship # &nbsp;&nbsp;
						stat_label(id='relations')
				.block_tree
					.vertical_spacing1
						script(type="text/javascript", src="/apollo/app/d3/d3-treemap.js")
				// end block_edge
			span.visible-lg.visible-md
					.legend
						table.legend_details
							tr
								td.legend_td
									.blue_subhead Legend:
								td.node_legend_image_td
									img.move_legend(src='img/node_ss.png')
